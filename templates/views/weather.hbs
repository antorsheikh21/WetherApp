<!doctype html>
<html lang="en">

<head>

    {{>headerLinks}}

</head>

<body>

    <!-- navbar starting -->

    {{>navBar}}

    <!-- main header -->


    <div class="container-fluid main_header mt-5">
        <div class="row">
            <div class="col-md-10 mx-auto col-12">

                <div class="row">
                    <!-- left side div -->
                    <div class="col-md-6 mx-auto">

                        <div class="main_content">
                            <form class="temp_form">
                                <input class="form-control" type="text" id="cityName"
                                    placeholder="Enter your city name " autocomplete="off">
                                <input type="submit" value="Search" id="submitBtn">
                            </form>
                        </div>

                        <div class="tempInformation mt-5">
                            <div class="top_leyar">
                                <p id="day">Tuesday</p>
                                <p id="today_data">20 jy</p>
                            </div>
                            <div class="main_layer">
                                <p class="text-center" id="city_name"> Get output Here</p>
                                <div class="midle_layer data_hide">
                                    <p id="temp"> <span id="temp_real_val">0</span> <sup>o</sup>C</p>
                                    <p id="temp_status"> <i class="fa fa-cloud aria-hidden=" true"></i></p>
                                </div>
                            </div>
                        </div>

                    </div>

                </div>


            </div>

        </div>
    </div>




    <!-- Footer code -->
 {{>icons}}
    {{>footer}}

    <script src="js/main.js"></script>

    <script src="js/date.js"> </script>

</body>

</html>